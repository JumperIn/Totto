@model MainPage
@{
    ViewBag.Seo = Model.Seo;
    ViewBag.Navigation = Model.Navigation;
}

@await Html.PartialAsync("_Banners", Model.Banners)
@await Html.PartialAsync("_Products", Model.Products)
@await Html.PartialAsync("_Discounts", Model.Promos)
@await Html.PartialAsync("_NewProducts", Model.PromoProducts)

<script src="~/js/main.js" asp-append-version="true"></script>